#include<stdio.h>
int main(){ 
   int array[5],i;
   
   printf("Enter elements into the array: \n");
  
   for(i=0;i<5;i++){
      
      printf("array[%d] :",i);
      scanf("%d",&array[i]);
   }
  
   printf("The elements from the array displayed in the reverse order are :\n");
   for(i=4;i>=0;i--){
     
      printf("array[%d] :",i);
      printf("%d\n",array[i]);
   }
}
